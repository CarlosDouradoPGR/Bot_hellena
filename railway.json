{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pip install --upgrade pip setuptools wheel && pip install -r requirements.txt"
  },
  "deploy": {
    "startCommand": "streamlit run dashboard.py --server.port $PORT --server.enableCORS false --server.enableXsrfProtection false"
  }
}
